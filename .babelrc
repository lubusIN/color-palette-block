{
  "presets": [
    [
      "env",
      {
        "modules": false
      }
    ]
  ],
  "plugins": [
		"lodash",
    "transform-object-rest-spread",
    [
      "transform-react-jsx",
      {
        "pragma": "wp.element.createElement"
      }
		]
  ],
  "env": {
    "default": {
      "plugins": ["transform-runtime"]
    }
  }
}
